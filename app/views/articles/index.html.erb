<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <% @articles.each do |article| %>
        <h2>
          <a><%= link_to article.title, article_path(article) %></a>
        </h2>
        <p> <%= article.content %> </p>
      <% end %>
    </div>
  </div>
</div>
